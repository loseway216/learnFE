import { defineStore } from "pinia";
import http from "../utils/http";

export const useTopoStore = defineStore("topo", {
  state: () => ({
    topoData: {},
  }),
  actions: {
    async getData() {
      try {
        const res = await http.get(`/isoc/api/v1/asset_topo/get_asset_topo`);
        this.topoData = res.errMsg.data;
      } catch (error) {
        console.log(error);
      }
    },
  },
});
